
<% provide(:title, 'Attend Index') %>
<h1>出勤中社員一覧</h1>
<div>
  <table class="table table-bordered table-condensed" id="table-attendances">
    <thead> 
      <tr>
        <th> 社員番号</th>
        <th> 名前 </th>
      </tr>
    
      <% @users.each do |user| %>
      <tr>
        <th><%=user.uid %></th>
        <th><%=user.name %></th>
      </tr>
      <% end %>
    </thead>
  </table>
</div>
